{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "Observability": {
    "Elastic": {
      "Enabled": false,
      "Uri": "http://localhost:9200/",
      "Username": "elastic",
      "Password": "pass@123"
    },
    "ElasticApm": {
      "Enabled": false,
      "ServerUrl": "http://localhost:8200",
      "LogLevel": "Debug",
      "SecretToken": "",
      "TransactionSampleRate": 1.0,
      "CaptureBody": "all",
      "CaptureBodyContentTypes": "application/x-www-form-urlencoded*, text/*, application/json*, application/xml*"
    }
  },

  "AllowedHosts": "*"
}
